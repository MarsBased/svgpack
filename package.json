{
  "name": "@marsbased/svgpack",
  "version": "2.1.4",
  "description": "Convert SVG files into CSS variables",
  "repository": "git@github.com:MarsBased/svgpack.git",
  "author": "marsbased.com",
  "license": "MIT",
  "main": "src/index.ts",
  "types": "src/types.d.ts",
  "exports": {
    ".": "./src/index.ts",
    "./components": "./src/components/index.ts"
  },
  "bin": {
    "svgpack": "bin/svgpack"
  },
  "scripts": {
    "test": "jest",
    "test:eslint": "eslint src/",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "build": "tsc",
    "test:build": "npm run build && npm run test",
    "prepare": "node -e \"if(process.env.npm_config_global !== 'true' && !process.env.CI) { try { require('husky').install() } catch(e) { /* husky not available */ } }\""
  },
  "keywords": [
    "svg",
    "css",
    "variables",
    "css-variables",
    "scss",
    "sass",
    "icons",
    "images",
    "optimization",
    "build-tool"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "globby": "^11.1.0",
    "lodash": "^4.17.21",
    "minimist": "^1.2.8",
    "ora": "^5.4.1",
    "svgo": "^3.0.2",
    "to-slug-case": "^1.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.23.3",
    "@types/jest": "^29.5.0",
    "@types/react": ">=19.0.0",
    "eslint": "^8.56.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "react": ">=19.0.0"
  },
  "lint-staged": {
    "*.{js,json}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
